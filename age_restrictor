//AGE RESTRICTION

#include<iostream>
using namespace std;

int main() {
int age;
cout<<"enter age"<<endl;
cin>>age;

try{
    if (age>=18){
     
        cout<<"entry allowed";
    }
    else{
        throw(age);
    }
   
    }
catch(int e)
{
    cout<<"error because of"<<e;
}
}

/*/OUTPUT:
enter age
19
entry allowed
